{
  "name": "@piplup/monorepo",
  "version": "0.0.1",
  "license": "MIT",
  "homepage": "https://www.piplupjs.com/",
  "scripts": {
    "clean": "rimraf --glob ./packages/*/dist ./apps/storybook-host/storybook-static ./build-storybook.log",
    "lint": "nx run-many --target=lint --all",
    "build": "nx run-many --target=build --all",
    "build:packages": "nx run-many -t build -p tag:scope:packages",
    "build:storybook": "nx build-storybook storybook-host --verbose",
    "start:storybook": "nx storybook storybook-host",
    "reset": "nx reset",
    "prepare": "node ./scripts/prepare.mjs",
    "release": "npm run clean && npm run build && nx release"
  },
  "private": true,
  "devDependencies": {
