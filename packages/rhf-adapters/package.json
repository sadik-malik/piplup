    },
    "@types/react": {
      "optional": true
    },
    "mui-chips-input": {
      "optional": true
    },
    "mui-color-input": {
      "optional": true
    },
    "mui-file-input": {
      "optional": true
    },
    "mui-tel-input": {
      "optional": true
    },
    "mui-one-time-password-input": {
      "optional": true
    },
    "react-number-format": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
